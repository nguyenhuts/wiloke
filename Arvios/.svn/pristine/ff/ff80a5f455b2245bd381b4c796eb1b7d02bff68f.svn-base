  .navigation.nav-top.nav-fixed {
    -webkit-animation: none;
    -moz-animation: none;
    -ms-animation: none;
    -o-animation: none;
    animation: none;
  }

  .story .st-slogan {
    font-size: 18px;
  }
  .tags-wrap ~ .author-social {
    position: static;
    margin-top: 20px;
    -webkit-transform: translateY(0);
    -moz-transform: translateY(0);
    -ms-transform: translateY(0);
    -o-transform: translateY(0);
    transform: translateY(0);
  }
}
@media screen and (max-width: 767px) {
  .home-slider .home-media-content p {
    display: none;
  }

  header.header .head .logo {
    display: none;
  }
  header.header .head-social {
    margin-top: 15px;
    margin-bottom: 15px;
  }

  .search-box .search-form {
    right: -30px;
  }

  .story .st-slogan {
    font-size: 14px;
  }

  #comments .commentlist .comment-author {
    margin-right: 20px;
  }
  #comments .commentlist .comment .children {
    margin-left: 130px;
  }
}
@media screen and (max-width: 640px) {
  .about-author .image-thumb {
    width: 100px;
    height: 100px;
    margin-right: 20px;
  }
  .about-author .author-info {
    overflow: visible;
  }
  .about-author .author-info .author-name h3 {
    font-size: 20px;
  }

  #comments .commentlist .comment-author {
    width: 80px;
    height: 80px;
  }
  #comments .commentlist .comment .children {
    margin-left: 50px;
  }
  #comments .commentlist .comment-box {
    overflow: visible;
  }

  #respond form .form-item {
    width: 100%;
  }

  .funfact [class*="col-"] {
    width: 100%;
    float: none;
  }

  .do-you-have-an-ideas .tb, .do-you-have-an-ideas .tb-cell {
    display: block;
  }
  .do-you-have-an-ideas .item-link {
    margin-top: 25px;
  }
}
@media screen and (max-width: 600px) {
  .admin-bar #page-wrap header.header .pi-navigation.nav-top.nav-fixed,
  .admin-bar #page-wrap header.header .pi-navigation.nav-bottom.nav-fixed {
    top: 0;
    bottom: auto;
  }

}
@media screen and (max-width: 480px) {
  #filters ul li {
    display: block;
  }
  #filters ul li .h-btn,
  #filters ul li a {
    display: block;
  }

  #comments .commentlist .comment .children {
    margin-left: 20px;
  }

  .about-author {
    padding: 15px;
  }
  .about-author .image-thumb {
    width: 80px;
    height: 80px;
  }
}
/* Fix background cover safari */
@media only screen and (orientation: portrait) and (device-width: 320px), (device-width: 768px) {
  html .awe-parallax, html .awe-static, .home-slider li .image-wrap .item-img {
    -webkit-background-size: auto 150%  !important;
    background-attachment: scroll  !important;
  }
}
@media only screen and (orientation: landscape) and (device-width: 320px), (device-width: 768px) {
  html .awe-parallax, html .awe-static, .home-slider li .image-wrap .item-img {
    -webkit-background-size: 150% auto  !important;
    background-attachment: scroll  !important;
  }
}
