#TB_window {
	background-color: #fff;
	border: 1px solid #999;
	border: 1px solid rgba(0,0,0,.2);
	border-radius: 6px;
	outline: 0;
	-webkit-box-shadow: 0 3px 9px rgba(0,0,0,.5);
	box-shadow: 0 3px 9px rgba(0,0,0,.5);
}
#TB_window h3 {
	border-bottom: 1px solid #e0e0e0 !important;
	padding-bottom: 10px !important;
}
#TB_title {
	border-top-left-radius: 6px;
	border-top-right-radius: 6px;
	padding-top: 5px;
	padding-bottom: 5px;
	font-size: 20px;
}
.tb-close-icon {
	top: 4px;
	right: 4px;
}
.bo-tb-fa li {
	border: 0 !important;
}
.bo-tb-fa li:hover, .bo-tb-fa li:hover i {
	-webkit-transform: scale(1.5) !important;
	-moz-transform: scale(1.5) !important;
	transform: scale(1.5) !important;
	color: #d42020 !important;
}
#TB_ajaxContent h2 {font-weight: 600;font-size: 28px;margin-bottom: 20px;}
.bo-tb-fa li {
    float: left;
    text-align: center;
    width: 47px;
    border: 1px solid #E1E1E1;
    margin-left: -1px;
    margin-top: -1px;
    transition: all 0.1s ease-out 0s;
    background: #FFF;
    cursor: pointer;
    margin-bottom: 0;
}
.bo-tb-fa li i {
    color: #494949;
    font-size: 22px;
    line-height: 45px;
    transition: all 0.1s ease-out 0s;   
}
.bo-tb-fa li:hover, 
.bo-tb-fa li:hover i {
    transform: scale(1.2);
    color: #278AB7;
}
.bo-icon-choosen i{
    transform: scale(1.2);
    color: #D54E21;
}
#bo-metro-zero-tb-fa > h3, #table-fa h3 {
    font-weight: 400;
    font-size: 15px;
    color: #555555;
    clear: both;
    margin: 20px 0;
}
.h190 {
    overflow-x: hidden;
    overflow-y: auto;
    height: 240px;
    width: 100%;
    border: 1px solid #E1E1E1;
}

/*nav tab*/
.nav-tabs {
  border-bottom: 1px solid #dddddd;
}

.nav-tabs > li {
  float: left;
  margin-bottom: -1px;
}

.nav-tabs > li > a {
  margin-right: 2px;
  line-height: 1.428571429;
  border: 1px solid transparent;
  border-radius: 4px 4px 0 0;
}

.nav-tabs > li > a:hover {
  border-color: #eeeeee #eeeeee #dddddd;
}

.nav-tabs > li.active > a,
.nav-tabs > li.active > a:hover,
.nav-tabs > li.active > a:focus {
  color: #555555;
  cursor: default;
  background-color: #ffffff;
  border: 1px solid #dddddd;
  border-bottom-color: transparent;
}

.nav-tabs.nav-justified {
  width: 100%;
  border-bottom: 0;
}

.nav-tabs.nav-justified > li {
  float: none;
}

.nav-tabs.nav-justified > li > a {
  margin-bottom: 5px;
  text-align: center;
}

.nav-tabs.nav-justified > .dropdown .dropdown-menu {
  top: auto;
  left: auto;
}

@media (min-width: 768px) {
  .nav-tabs.nav-justified > li {
    display: table-cell;
    width: 1%;
  }
  .nav-tabs.nav-justified > li > a {
    margin-bottom: 0;
  }
}

.nav-tabs.nav-justified > li > a {
  margin-right: 0;
  border-radius: 4px;
}

.nav-tabs.nav-justified > .active > a,
.nav-tabs.nav-justified > .active > a:hover,
.nav-tabs.nav-justified > .active > a:focus {
  border: 1px solid #dddddd;
}

@media (min-width: 768px) {
  .nav-tabs.nav-justified > li > a {
    border-bottom: 1px solid #dddddd;
    border-radius: 4px 4px 0 0;
  }
  .nav-tabs.nav-justified > .active > a,
  .nav-tabs.nav-justified > .active > a:hover,
  .nav-tabs.nav-justified > .active > a:focus {
    border-bottom-color: #ffffff;
  }
}

.pi_contactinfo {
  position: relative;
}
.pi_contactinfo > .js_add_icon {
  display: inline-block;
  width: 40px; height: 40px;
  text-align: center;
  line-height: 40px;
  font-size: 16px;
  background-color: #eee;
  position: absolute;
  top: 0; left: 0;
}
.pi_contactinfo input {
  padding-left: 55px !important;
  padding-right: 55px !important;
  width: 100% !important;
  box-shadow: none !important;
}
.pi_contactinfo > .js_delete_item {
  display: inline-block;
  width: 40px; height: 40px;
  text-align: center;
  line-height: 40px;
  font-size: 16px;
  color: red;
  background-color: transparent;
  position: absolute;
  top: 0; right: 0;
}
.pi-fontawesome {
  padding: 20px;
  padding-top: 0;
}
.pi-fontawesome .bo-tb-fa li:hover, .bo-tb-fa li:hover i {
    color: #278ab7;
    transform: scale(1) !important;
}
.pi-fontawesome .bo-icon-choosen i {
    color: #d54e21;
    transform: scale(1) !important;
}
.pi-fontawesome .h190 {
  overflow: visible;
  border: 0;
  height: auto;
}
.pi-fontawesome .bo-tb-fa {
  height: 200px;
  overflow-x: hidden;
  overflow-y: visible;
  border: 1px solid #d4d4d4;
}
.pi-popup .pi-footer {
  position: static !important;
  overflow: hidden;
  text-align: left;
  padding: 0 20px;
}
.pi-popup .pi-footer button {
  margin-left: 5px;
}

.btn.h-btn {
  display: inline-block;
  outline: none;
  font-family: "Raleway", cursive;
  font-size: 13px;
  font-weight: 600;
  background: none;
  color: #fff;
  border: 0;
  background-color: #00c8ff;
  text-align: center;
  text-shadow: none;
  box-shadow: none;
  text-indent: 1px;
  padding: 10px 20px;
  letter-spacing: 0.1em;
  text-transform: uppercase;
  white-space: nowrap;
  -webkit-transition: all 0.3s ease;
  -moz-transition: all 0.3s ease;
  -ms-transition: all 0.3s ease;
  -o-transition: all 0.3s ease;
  transition: all 0.3s ease;
}
.btn.h-btn:hover {
  background-color: #222;
  color: #fff;
}
.btn.h-btn{
  border-radius: 0px;
}