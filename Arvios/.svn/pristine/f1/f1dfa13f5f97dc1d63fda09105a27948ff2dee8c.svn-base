}*/

/*========= Layout Styles =========*/
.pi-popup {
    background: none repeat scroll 0 0 #FFFFFF;
    bottom: 30px;
    left: 15%;
    min-height: 210px;
    overflow: auto;
    position: fixed;
    right: 15%;
    top: 10%;
    z-index: 1400;

}
.pi-wrap-textarea{
    padding: 20px;
}
.pi-popup-inner-header {
    border-bottom: 1px solid #DFDFDF;
    height: 50px;
    position: relative;
}
.pi-popup-inner-content {
    bottom: 50px;
    left: 0;
    overflow: auto;
    position: absolute;
    right: 0;
    top: 51px;
    width: 100%;
}
.pi-popup-inner-footer {
    background: #FCFCFC;
    border-top: 1px solid #dfdfdf;
    bottom: 0;
    height: 50px;
    width: 100%;
    position: absolute;
}
.pi-popup-backdrop.js_backdrop 
{
    background: #000;
    bottom: 0;
    left: 0;
    min-height: 400px;
    opacity: 0.7;
    position: fixed;
    right: 0;
    top: 0;
    z-index: 1200;
}

/* Dimension popup screen */
.pi-popup.popup-big {
    bottom: 2%;
    left: 2%;
    right: 2%;
}
.pi-popup.popup-medium {
    bottom: 20%;
    left: 20%;
    right: 20%;
    top: 20%;
}
.pi-popup.popup-small {
    bottom: auto;
    left: 30%;
    right: 30%;
    top: 35%;
}

/*========= Header Styles =========*/
.pi-popup-title {
    color: #555;
    font-size: 22px;
    font-weight: 200;
    left: 14px;
    line-height: 53px;
    position: absolute;
    top: 0;
    margin: 0 !important;
}
.pi-popup-close {
    background: url("images/icon-close.png") no-repeat left top;
    height: 25px;
    overflow: hidden;
    position: absolute;
    right: 14px;
    text-indent: 300%;
    top: 14px;
    width: 25px;

}
.pi-popup-close:hover {
    background-position: 0 -25px;
}
/*========= Content Styles =========*/
.pi-popup .pi-row-item {
    background: #FFF;
    border-bottom: 1px dashed #E1E1E1;
    padding: 20px;
}
.pi-popup .pi-row-item:hover {
    background-color: #FCFCFC;
}
/*========= Footer Styles =========*/
.pi-popup-inner-footer .button-large {
    padding: 0 40px 2px;
    position: absolute;
    right: 12px;
    top: 10px;
}

.pi-popup .pi-row-item,
.pi-popup .row-inner {
    display: table;
    width: 100%;
}
.pi-popup .row-inner {
    padding: 10px 0px;  
}
.pi-popup .pi-row-item .pi-name , 
.pi-popup .pi-row-item .pi-setting  {
        display: table-cell;
        vertical-align: middle;
        min-height: 50px;
    }
.pi-name{
    width: 45%;
}
.pi-name p {
    color: #939292;
    display: block;
    font-size: 14px;
    font-style: italic;
    line-height: 21px;
}
.pi-setting {
    width: 55%;
}
.pi-popup .pi-row-item .pi-setting  input[type="text"],
.pi-popup .pi-row-item .pi-setting  textarea {
    background-color: #fff;
    border: 1px solid #9ca0a3;
    box-shadow: none;
    color: #333;
    font: 12px/20px Arial,Helvetica,sans-serif;
    padding: 6px 15px;
    width: 100%;
    border-radius: 0 !important;
    margin-bottom: 10px;
}
.pi-popup .wp-picker-container input[type="text"] {
    margin-bottom: 0 !important;
    height: 27px;
    margin-left: 20px;
    width: 100px !important;
}

.pi-popup .pi-row-item .pi-setting .pi-medium{
    width: 45% !important
}



.pi-popup .pi-row-item  .pi-name p > code {
    color: #f00;
    background: transparent;
}
/*========= Box grid Styles =========*/
.pi-box-grid {
    float: left;
    margin: 1%;
    position: relative;
    width: 200px;
}
.pi-box-header {
    height: 25px;
    opacity: 1;
    position: relative;
    width: auto;
    z-index: 10;
    background: none repeat scroll 0 0 #FAFAFA;
    color: #222222;
    border: 1px solid #E5E5E5;
}
.pi-box-shortcode {
    background: none repeat scroll 0 0 #F8F8F8;
    border: 1px dashed #E1E1E1;
    min-height: 35px;
    padding: 20px 10px;
    position: relative;
    top: -5px;
}
.pi-box-header > [class^="toogle-"],
.pi-box-header > [class*="toogle-"] {
    display: block;
    float: left;
    height: 24px;
    opacity: 0;
    overflow: hidden;
    position: relative;
    width: 24px;
    color: #666;
    font-size: 20px;
    margin-right: 10px


}
.pi-box-grid:hover  [class^="toogle-"],
.pi-box-grid:hover  [class*="toogle-"]{
    opacity: 1;
}
.grid-size {
    color: #999999;
    float: left;
    height: 24px;
    left: -30px;
    line-height: 28px;
    position: relative;
    text-align: center;
    width: 24px;

}



.js_center{
    bottom: 0;
    height: 60px;
    left: 0;
    margin: auto;
    position: absolute;
    right: 0;
    text-align: center;
    top: 0;
    width: 60px;
}

.child-item{
    width: 100%;
}

