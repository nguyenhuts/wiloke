* {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
/*Toggle*/

#pi_popup h3.hndle{
    font-weight: normal !important;
    padding: 9px 1.1% !important;
    font-size: 14px;
    line-height: 1.4;
    margin: 0;
    padding: 8px 12px; 
    border-bottom: 1px solid #eee;
    cursor: pointer;
    border: 1px solid #e5e5e5;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.04);
    margin-top: 32px;
}
#pi_popup h3.hndle:after{
    content: "\f343";
    font-size: 13px;
    font-family: "dashicons";
    line-height: 20px;
    position: absolute;
    right: 30px;
}

#pi_popup h3.hndle.closed:after{
    content:  "\f347";
}

.pi_setting{
    border-right: 1px solid #ddd;
    border-left: 1px solid #ddd;
    border-bottom: 1px solid #ddd;
}


/*========= Animation library Styles =========*/
@-webkit-keyframes pi-popup {
    0% {
        opacity: 0;
        -webkit-transform: scale(0.8);
        -moz-transform: scale(0.8);
        -ms-transform: scale(0.8);
        -o-transform: scale(0.8);
        transform: scale(0.8);
    }
    100% {
        opacity: 1;
        -webkit-transform: scale(1);
        -moz-transform: scale(1);
        -ms-transform: scale(1);
        -o-transform: scale(1);
        transform: scale(1);
    }
}
@-moz-keyframes pi-popup {
    0% {
        opacity: 0;
        -webkit-transform: scale(0.8);
        -moz-transform: scale(0.8);
        -ms-transform: scale(0.8);
        -o-transform: scale(0.8);
        transform: scale(0.8);
    }
    100% {
        opacity: 1;
        -webkit-transform: scale(1);
        -moz-transform: scale(1);
        -ms-transform: scale(1);
        -o-transform: scale(1);
        transform: scale(1);
    }
}
@-ms-keyframes pi-popup {
    0% {
        opacity: 0;
        -webkit-transform: scale(0.8);
        -moz-transform: scale(0.8);
        -ms-transform: scale(0.8);
        -o-transform: scale(0.8);
        transform: scale(0.8);
    }
    100% {
        opacity: 1;
        -webkit-transform: scale(1);
        -moz-transform: scale(1);
        -ms-transform: scale(1);
        -o-transform: scale(1);
        transform: scale(1);
        
    }
}
@-o-keyframes pi-popup {
    0% {
        opacity: 0;
        -webkit-transform: scale(0.8);
        -moz-transform: scale(0.8);
        -ms-transform: scale(0.8);
        -o-transform: scale(0.8);
        transform: scale(0.8);
    }
    100% {
        opacity: 1;
        -webkit-transform: scale(1);
        -moz-transform: scale(1);
        -ms-transform: scale(1);
        -o-transform: scale(1);
        transform: scale(1);
    }
}
@keyframes pi-popup {
    0% {
        opacity: 0;
        -webkit-transform: scale(0.8);
        -moz-transform: scale(0.8);
        -ms-transform: scale(0.8);
        -o-transform: scale(0.8);
        transform: scale(0.8);
    }
    100% {
        opacity: 1;
        -webkit-transform: scale(1);
        -moz-transform: scale(1);
        -ms-transform: scale(1);
        -o-transform: scale(1);
        transform: scale(1);
    }
}

.pi-popup, 
.pi-box-grid {
    -webkit-animation: .5s cubic-bezier(0.680, -0.550, 0.265, 2) 0s  1 pi-popup;
    -moz-animation: .5s cubic-bezier(0.680, -0.550, 0.265, 2) 0s  1 pi-popup;
    -o-animation: .5s cubic-bezier(0.680, -0.550, 0.265, 2) 0s 1 pi-popup;
    animation: .5s cubic-bezier(0.680, -0.550, 0.265, 2) 0s 1 pi-popup;
}
a,
.grid-size,
.pi-box-header > [class^="toogle-"],
.pi-box-header > [class*="toogle-"],
.pi-popup .pi-row-item,
.pi-popup-close  {
    -webkit-transition: all .2s ease 0s;
    -moz-transition: all .2s ease 0s;
    -ms-transition: all .2s ease 0s;
    -o-transition: all .2s ease 0s;
    transition: all .2s ease 0s;
}

