	function pi_change_background_color(section, i)
	{
		var key = 'pi_save_theme_options[theme_options]['+section+'][bg_color]';
		wp.customize( key, function(value)
		{
			value.bind(function(newval)
			{
				$("#"+section).css("background-color", newval);
				$("#"+section).find(".bg-parallax, .bg-overlay").css({display: "none"});
				$("#"+section).find(".bg-parallax").removeClass("bg-bg-parallax");	
			})
			
		});
	}

	function pi_background_overlay(section, i)
	{
		var key = 'pi_save_theme_options[theme_options]['+section+'][overlay]';
		wp.customize( key, function(value)
		{
			value.bind(function(newval)
			{
				if ( newval == 1 )
				{
					if ( $("#"+section +" .bg-overlay").length>0 )
					{
						$("#"+section + " .bg-overlay").css({display:"block"});
					}else{
						$("#"+section + " .bg-parallax").after('<div class="bg-overlay"></div>');
					}

					$("#"+section).css('background-color', '');
				}else{
					$("#"+section + " .bg-overlay").css({display:"none"});
				}

			})
			
		});
	}

	function pi_background_img(section, i)
	{

		var key = 'pi_save_theme_options[theme_options]['+section+'][bg_img]';
		wp.customize( key, function(value)
		{
			value.bind(function(newval)
			{
				if($("#"+section + " .bg-parallax").length >  0 )
				{
					$("#"+section + " .bg-parallax").css({'background-image': 'url('+newval+')'});
				}else{
					$("#"+section).prepend('<div  class="bg-parallax bg-bg-parallax" style="background-image:url('+newval+')"></div>');
				}
				$("#"+section).css('background-color', '');
			})
			
		});
	}

	function pi_live_preview_contact_info(key)
	{
		var $concat = '['+key+']';
		if ( key != 'email' && key != 'phone' && key != 'mobile' && key !='address' )
		{
			$concat = '[social]['+key+']';
		}

		var $target=$("#map_wrap .info_block"), $html="", $insert;
		wp.customize( 'pi_save_theme_options[theme_options][contact]'+$concat, function(value)
		{
			value.bind(function(newval)
			{
				switch (key)
				{
					case 'email':
					if ( $target.find(".fa-envelope").length>0 )
					{
						$insert = $target.find(".fa-envelope").next();
						$insert.attr("href", 'mailto:'+ newval);
						$insert.html(newval);
					}else{
						$target.append('<span class="fa fa-envelope"></span><a href="mailto:'+newval+'">info@company.com</a><br><br>');
					}
					break;

					case 'phone':
					if ( $target.find(".fa-phone").length>0 )
					{
						$insert = $target.find(".fa-phone");
						$insert.after(newval);
					}else{
						$target.append('<span class="fa fa-phone"></span>'+newval+'<br/><br/>');
					}
					break;

					case 'mobile':
					if ( $target.find(".fa-mobile").length>0 )
					{
						$insert = $target.find(".fa-mobile");
						$insert.after(newval);
					}else{
						$target.append('<span class="fa fa-mobile"></span>'+newval+'<br/><br/>');
					}
					break;

					case 'address':
					if ( $target.find(".address").length>0 )
					{
						$insert = $target.find(".address");
						$insert.html(newval);
					}else{
						$target.append('<span class="fa fa-home"></span><p class="address">'+newval+'</p>');
					}
					break;

					default:
					if ( $target.find(".pi-social-zone").length>0 )
					{
						if ( $target.find(".fa-"+key).length>0 )
						{
							$target.find(".fa-"+key).attr("href", newval);
						}else{
							$target.find(".pi-social-zone").append('<a target="_blank" class="fa fa-'+key+'" href="'+newval+'" title="'+key+'">#</a>');
						}
					}else{
						$target.append('<p class="pi-social-zone"><a target="_blank" class="fa fa-'+key+'" href="'+newval+'" title="'+key+'">#</a></p>');
					}
					break;
				}
			})
			
		});
	}

	function pi_change_section_bg_type(section, i)
	{
		var key = "pi_save_theme_options[theme_options]["+section+"][hack_section_bg]";
		wp.customize( key, function(value)
		{
			value.bind(function(newval)
			{	
				if ( newval != '' )
				{
					_jsonData = JSON.parse(decodeURIComponent(newval));
					
					if ( _jsonData.type == 'image' )
					{
						if ( $("#"+section + " .bg-parallax").length>0  )
						{
							$("#"+section + " .bg-parallax").css({display:'block'}).addClass("bg-bg-parallax");
						}else{
							$("#"+section).prepend('<div  class="bg-parallax bg-bg-parallax" style="background-image:url('+_jsonData.image+')"></div>');
						}
						if ( _jsonData.overlay == 1 )
						{
							if($("#"+section + " .bg-overlay").length>0)
							{
								$("#"+section + " .bg-overlay").css({display:'block'}).addClass("bg-bg-parallax");
							}else{
								$("#"+section + " .bg-parallax").after('<div class="bg-overlay"></div>');
							}
						}
						$("#"+section).css('background-color', '');
					}else if ( _jsonData.type == 'color' )
					{
						$("#"+section).css("background-color", _jsonData.color);
						$("#"+section).find(".bg-parallax, .bg-overlay").css({display: "none"}).removeClass("bg-bg-parallax");
					}else{
						$("#"+section).css("background-color","#fff");
						$("#"+section).find(".bg-parallax, .bg-overlay").css({display: "none"}).removeClass("bg-bg-parallax");
					}
				}
			})
			
		});
	}




