  -moz-border-radius: 0;
  -ms-border-radius: 0;
  -o-border-radius: 0;
  border-radius: 0;
}

.pi_tabs .ui-tabs-nav .ui-state-default .ui-tabs-anchor,
.pi_tabs .ui-tabs-nav .ui-state-default.ui-state-active .ui-tabs-anchor {
	background-color: #fff;
	border: 1px solid #edf2f2;
