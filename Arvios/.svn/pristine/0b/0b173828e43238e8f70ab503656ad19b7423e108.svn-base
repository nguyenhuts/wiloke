/* 14.   Preloader
--------------------------------------------------------------------------------*/
.preloader {
  position: fixed;
  width: 100%;
  height: 100%;
  background-color: #fff;
  z-index: 99999999;
  -webkit-transition: all 1s linear;
  -moz-transition: all 1s linear;
  -ms-transition: all 1s linear;
  -o-transition: all 1s linear;
  transition: all 1s linear;
}
.preloader .inner {
  position: absolute;
  width: 66px;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  -moz-transform: translate(-50%, -50%);
  -ms-transform: translate(-50%, -50%);
  -o-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
}
.preloader .ball {
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  -ms-border-radius: 50%;
  -o-border-radius: 50%;
  border-radius: 50%;
  background-color: #e75f4d;
  width: 14px;
  height: 14px;
  position: absolute;
  top: 0;
  left: 0;
  -webkit-animation: loading 1.2s linear infinite;
  -moz-animation: loading 1.2s linear infinite;
  animation: loading 1.2s linear infinite;
}
.preloader .ball:nth-of-type(2) {
  -webkit-animation-delay: 0.4s;
  -moz-animation-delay: 0.4s;
  animation-delay: 0.4s;
}
.preloader .ball:nth-of-type(3) {
  -webkit-animation-delay: 0.8s;
  -moz-animation-delay: 0.8s;
  animation-delay: 0.8s;
}
.preloader.load {
  opacity: 0;
  visibility: hidden;
  -webkit-transform: perspective(100em) rotateY(180deg) scale(0.5);
  -moz-transform: perspective(100em) rotateY(180deg) scale(0.5);
  -ms-transform: perspective(100em) rotateY(180deg) scale(0.5);
  -o-transform: perspective(100em) rotateY(180deg) scale(0.5);
  transform: perspective(100em) rotateY(180deg) scale(0.5);
}

@-webkit-keyframes loading {
  0% {
    opacity: 0;
    -webkit-transform: scale(0.5);
  }

  20% {
    opacity: 1;
    -webkit-transform: scale(1);
  }

  40% {
    -webkit-transform: translate3d(22px, 0, 0);
  }

  60% {
    -webkit-transform: translate3d(44px, 0, 0);
  }

  80% {
    -webkit-transform: translate3d(66px, 0, 0);
    opacity: 1;
  }

  100% {
    -webkit-transform: translate3d(66px, 0, 0) scale(0.5);
    opacity: 0;
  }
}
@-moz-keyframes loading {
  0% {
    opacity: 0;
    -moz-transform: scale(0.5);
  }

  20% {
    opacity: 1;
    -moz-transform: scale(1);
  }

  40% {
    -moz-transform: translate3d(22px, 0, 0);
  }

  60% {
    -moz-transform: translate3d(44px, 0, 0);
  }

  80% {
    -moz-transform: translate3d(66px, 0, 0);
    opacity: 1;
  }

  100% {
    -moz-transform: translate3d(66px, 0, 0) scale(0.5);
    opacity: 0;
  }
}
@keyframes loading {
  0% {
    opacity: 0;
    transform: scale(0.5);
  }

  20% {
    opacity: 1;
    transform: scale(1);
  }

  40% {
    transform: translate3d(22px, 0, 0);
  }

  60% {
    transform: translate3d(44px, 0, 0);
  }

  80% {
    transform: translate3d(66px, 0, 0);
    opacity: 1;
  }

  100% {
    transform: translate3d(66px, 0, 0) scale(0.5);
    opacity: 0;
  }
}
