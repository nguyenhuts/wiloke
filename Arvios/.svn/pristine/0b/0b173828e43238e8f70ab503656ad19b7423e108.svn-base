header.header {
  position: relative;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  -webkit-transition: all 0.3s ease;
  -moz-transition: all 0.3s ease;
  -ms-transition: all 0.3s ease;
  -o-transition: all 0.3s ease;
  transition: all 0.3s ease;
}
header.header .head {
  position: absolute;
  z-index: 9999;
  width: 100%;
  top: 0;
  left: 0;
  padding-top: 5.3em;
}
header.header .head-social {
  margin-top: 45px;
  margin-bottom: 20px;
}
header.header .head-social a .fa {
  color: #fff;
  border-right-color: rgba(255, 255, 255, 0.2);
  border-bottom-color: rgba(255, 255, 255, 0.2);
  -webkit-transition: all 0.3s ease;
  -moz-transition: all 0.3s ease;
  -ms-transition: all 0.3s ease;
  -o-transition: all 0.3s ease;
  transition: all 0.3s ease;
}
header.header .head-social a:before, header.header .head-social a:after {
  background-color: rgba(255, 255, 255, 0.2);
  -webkit-transition: all 0.3s ease;
  -moz-transition: all 0.3s ease;
  -ms-transition: all 0.3s ease;
  -o-transition: all 0.3s ease;
  transition: all 0.3s ease;
}
header.header .head-social a:hover .fa {
  color: #e75f4d;
  border-right-color: #e75f4d;
  border-bottom-color: #e75f4d;
}
header.header .head-social a:hover:before, header.header .head-social a:hover:after {
  background-color: #e75f4d;
}

.home-media {
  position: relative;
  overflow: hidden;
}
.home-media .text-content {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 9;
}
.home-media .tb {
  height: 100%;
}
.home-media .home-media-content {
  position: relative;
  max-width: 950px;
  margin: auto;
  padding: 0 60px;
  color: #fff;
  z-index: 999;
}
.home-media .home-media-content * {
  color: inherit;
}
.home-media .home-media-content .h1 {
  font-family: "Satisfy", cursive;
}
.home-media .home-media-content .he-divider {
  background-color: rgba(255, 255, 255, 0.3);
  margin-bottom: 30px;
}

.home-slider .tb {
  width: 100%;
  height: 100%;
}
.home-slider .item .bg-overlay {
  background-color: rgba(71, 71, 71, 0.3);
}

/* bg video */
.bg-video {
  width: 100%;
  height: 100%;
  z-index: 0;
}
.bg-video ~ .bg-overlay {
  z-index: 2;
}

.controls-video span {
  background: none;
  cursor: pointer;
  display: block;
}
.controls-video .play {
  position: absolute;
  width: 60px;
  height: 60px;
  margin: auto;
  left: 0;
  right: 0;
  bottom: 80px;
  z-index: 99999;
}
.controls-video .play .fa {
  display: block;
  font-size: 18px;
  line-height: 58px;
  text-align: center;
  width: 60px;
  height: 60px;
  border: 1px solid #e75f4d;
  color: #e75f4d;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
  -webkit-transition: all .3s ease;
  -moz-transition: all .3s ease;
  -ms-transition: all .3s ease;
  -o-transition: all .3s ease;
  transition: all .3s ease;
}
.controls-video .play .fa:hover {
  background-color: rgba(225, 225, 225, 0.4);
}

.controls-sm {
  position: absolute;
  text-align: center;
  left: 50%;
  bottom: 80px;
  overflow: hidden;
  z-index: 99999;
  -webkit-transform: translateX(-50%);
  -moz-transform: translateX(-50%);
  -ms-transform: translateX(-50%);
  -o-transform: translateX(-50%);
  transform: translateX(-50%);
}
.controls-sm span {
  display: inline-block;
  margin: 0 5px;
  float: left;
}
.controls-sm span .fa {
  display: inline-block;
  text-align: center;
  line-height: 30px;
  width: 30px;
  height: 30px;
  border: 1px solid #e75f4d;
  color: #e75f4d;
  font-size: 14px;
  opacity: .5;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  -ms-border-radius: 50%;
  -o-border-radius: 50%;
  border-radius: 50%;
  -webkit-transition: all 0.3s ease;
  -moz-transition: all 0.3s ease;
  -ms-transition: all 0.3s ease;
  -o-transition: all 0.3s ease;
  transition: all 0.3s ease;
}
.controls-sm span .fa:hover {
  opacity: 1;
}

.video-place {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 1;
}

.video-place, .bg-video {
  /*background-image: url("../img/bg/2.jpg");*/
  background-repeat: no-repeat;
  background-attachment: fixed;
  background-position: center center;
  -webkit-background-size: cover;
  -moz-background-size: cover;
  -ms-background-size: cover;
  -o-background-size: cover;
  background-size: cover;
}

/* 3.   Navigation
--------------------------------------------------------------------------------*/
.pi-navigation {
  position: absolute;
  width: 100%;
  border-bottom: 1px solid #e3e3e3;
  left: 0;
  height: 62px;
  background-color: #fff;
  z-index: 9999;
}
.pi-navigation.nav-bottom {
  bottom: 0;
}
.pi-navigation.nav-top {
  top: 0;
}
.pi-navigation.nav-top.nav-fixed {
  -webkit-animation: nav-anim 0.5s ease;
  -moz-animation: nav-anim 0.5s ease;
  -ms-animation: nav-anim 0.5s ease;
  -o-animation: nav-anim 0.5s ease;
  animation: nav-anim 0.5s ease;
}
.pi-navigation.nav-fixed {
  position: fixed;
  top: 0;
  bottom: auto;
}
header.header .head{
  z-index: 9999;
}
.header .logo .pi-text-logo{
  z-index: 99;
}
.pi-text-logo{
  cursor: pointer;
}
.pi-navigation .container {
  height: 100%;
}
.pi-navigation .logo-nav {
  display: inline-block;
  line-height: 60px;
}
.pi-navigation .logo-nav a {
  display: inline-block;
}
.pi-navigation .logo-nav a img {
  max-height: 25px;
}
.pi-navigation ul {
  list-style: none;
  margin: 0;
  padding: 0;
}
.pi-navigation .nav {
  position: absolute;
  font-size: 0;
  top: 0;
  right: 80px;
}
.pi-navigation .nav li {
  position: relative;
  display: inline-block;
  vertical-align: middle;
}
.pi-navigation .nav li a {
  position: relative;
  display: inline-block;
  font-family: "Concert One", cursive;
  font-size: 17px;
  color: #474747;
  background: none;
  border-right: 1px solid #e3e3e3;
  line-height: 62px;
  padding: 0 25px;
  -webkit-transition: all 0.3s linear;
  -moz-transition: all 0.3s linear;
  -ms-transition: all 0.3s linear;
  -o-transition: all 0.3s linear;
  transition: all 0.3s linear;
}
.pi-navigation .nav li a:after {
  content: '';
  display: block;
  position: absolute;
  width: 100%;
  height: 1px;
  background-color: transparent;
  left: 0;
  bottom: 0;
  -webkit-transition: all 0.3s linear;
  -moz-transition: all 0.3s linear;
  -ms-transition: all 0.3s linear;
  -o-transition: all 0.3s linear;
  transition: all 0.3s linear;
}
.pi-navigation .nav li:first-child a {
  font-size: 0;
  padding: 0 15px;
  border-left: 1px solid #e3e3e3;
}
.pi-navigation .nav li:first-child a .fa {
  position: relative;
  font-size: 17px;
  color: #474747;
  top: 5px;
}
.pi-navigation .nav li:hover > a {
  color: #e75f4d;
}
.pi-navigation .nav li:hover > a:after {
  background-color: #e75f4d;
}
.pi-navigation .nav li.active > a, .pi-navigation .nav li.current-menu-item > a {
  color: #e75f4d;
}
.pi-navigation .nav li.active > a .fa, .pi-navigation .nav li.current-menu-item > a .fa {
  color: inherit;
}
.pi-navigation .nav li.active > a:after, .pi-navigation .nav li.current-menu-item > a:after {
  background-color: #e75f4d;
}
.pi-navigation .nav .submenu-toggle,
.pi-navigation .nav .back-mb {
  display: none;
}
.pi-navigation .nav.nav-mobile {
  position: fixed;
  top: 0;
  right: 0;
  width: 280px;
  height: 100%;
  overflow-x: hidden;
  overflow-y: auto;
  background-color: #eee;
  -webkit-box-shadow: 0 100px 0 0 #eeeeee;
  -moz-box-shadow: 0 100px 0 0 #eeeeee;
  box-shadow: 0 100px 0 0 #eeeeee;
  padding: 40px 0;
  z-index: 9999;
  -webkit-transform: translateX(100%);
  -moz-transform: translateX(100%);
  -ms-transform: translateX(100%);
  -o-transform: translateX(100%);
  transform: translateX(100%);
  -webkit-transition: all 0.3s cubic-bezier(1, 0.01, 0.29, 0.93);
  -moz-transition: all 0.3s cubic-bezier(1, 0.01, 0.29, 0.93);
  -ms-transition: all 0.3s cubic-bezier(1, 0.01, 0.29, 0.93);
  -o-transition: all 0.3s cubic-bezier(1, 0.01, 0.29, 0.93);
  transition: all 0.3s cubic-bezier(1, 0.01, 0.29, 0.93);
}
.pi-navigation .nav.nav-mobile.nav-active {
  -webkit-transform: translateX(0);
  -moz-transform: translateX(0);
  -ms-transform: translateX(0);
  -o-transform: translateX(0);
  transform: translateX(0);
}
.pi-navigation .nav.nav-mobile.nav-active ~ .close-menu {
  opacity: 1;
  visibility: visible;
}
.pi-navigation .nav.nav-mobile .submenu-toggle,
.pi-navigation .nav.nav-mobile .back-mb {
  display: block;
}
.pi-navigation .nav.nav-mobile li {
  position: static;
  display: block;
}
.pi-navigation .nav.nav-mobile li a {
  border: 0;
  line-height: 40px;
  font-size: 14px;
  width: 100%;
  border-bottom: 1px solid rgba(0, 0, 0, 0.05);
}
.pi-navigation .nav.nav-mobile li a:after {
  display: none;
}
.pi-navigation .nav.nav-mobile li:first-child > a {
  padding: 0 25px;
}
.pi-navigation .nav.nav-mobile li:first-child > a .fa {
  font-size: 0;
  color: transparent;
}
.pi-navigation .nav.nav-mobile li .sub-menu {
  top: 0;
  width: 95%;
  height: 100%;
  overflow-x: hidden;
  overflow-y: auto;
  padding: 0 0 40px 0;
  background-color: #eee;
  opacity: 1;
  visibility: visible;
  top: -1px;
  right: 0;
  left: auto;
  z-index: 9;
  -webkit-transform: translateX(100%) rotateY(0);
  -moz-transform: translateX(100%) rotateY(0);
  -ms-transform: translateX(100%) rotateY(0);
  -o-transform: translateX(100%) rotateY(0);
  transform: translateX(100%) rotateY(0);
}
.pi-navigation .nav.nav-mobile li .sub-menu .left {
  right: 0;
  margin-left: 0;
}
.pi-navigation .nav.nav-mobile li .sub-menu .right {
  right: 0;
  margin-left: 0;
}
.pi-navigation .nav.nav-mobile li .sub-menu.translate {
  -webkit-transform: translateX(0) rotateY(0);
  -moz-transform: translateX(0) rotateY(0);
  -ms-transform: translateX(0) rotateY(0);
  -o-transform: translateX(0) rotateY(0);
  transform: translateX(0) rotateY(0);
  border: 1px solid rgba(0, 0, 0, 0.05);
  -webkit-box-shadow: 0 0 20px 0 rgba(0, 0, 0, 0.1);
  -moz-box-shadow: 0 0 20px 0 rgba(0, 0, 0, 0.1);
  box-shadow: 0 0 20px 0 rgba(0, 0, 0, 0.1);
}
.pi-navigation .open-menu {
  position: absolute;
  display: inline-block;
  width: 50px;
  height: 50px;
  margin: auto;
  top: 0;
  right: 0;
  bottom: 0;
}
.pi-navigation .open-menu .item {
  display: inline-block;
  position: absolute;
  display: block;
  font-size: 0;
  width: 20px;
  height: 2px;
  background-color: #474747;
  margin: auto;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  -webkit-transition: all 0.3s ease;
  -moz-transition: all 0.3s ease;
  -ms-transition: all 0.3s ease;
  -o-transition: all 0.3s ease;
  transition: all 0.3s ease;
}
.pi-navigation .open-menu .item:before, .pi-navigation .open-menu .item:after {
  content: '';
  display: block;
  position: absolute;
  width: 20px;
  height: 2px;
  background-color: #474747;
  left: 0;
  -webkit-transition: all 0.3s ease;
  -moz-transition: all 0.3s ease;
  -ms-transition: all 0.3s ease;
  -o-transition: all 0.3s ease;
  transition: all 0.3s ease;
}
.pi-navigation .open-menu .item:before {
  top: -6px;
}
.pi-navigation .open-menu .item:after {
  bottom: -6px;
}
.pi-navigation .open-menu:hover .item {
  background-color: #e75f4d;
}
.pi-navigation .open-menu:hover .item:before, .pi-navigation .open-menu:hover .item:after {
  background-color: #e75f4d;
}
.pi-navigation .close-menu {
  position: fixed;
  width: 40px;
  height: 40px;
  top: 0;
  right: 0;
  opacity: 0;
  visibility: hidden;
  -webkit-transition: all 0.3s ease;
  -moz-transition: all 0.3s ease;
  -ms-transition: all 0.3s ease;
  -o-transition: all 0.3s ease;
  transition: all 0.3s ease;
  z-index: 999999;
}
.pi-navigation .close-menu span {
  position: absolute;
  width: 15px;
  height: 15px;
  margin: auto;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  -webkit-transform: rotate(45deg);
  -moz-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  -o-transform: rotate(45deg);
  transform: rotate(45deg);
}
.pi-navigation .close-menu span:before, .pi-navigation .close-menu span:after {
  content: '';
  display: block;
  position: absolute;
  background-color: #474747;
  margin: auto;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
}
.pi-navigation .close-menu span:before {
  width: 1px;
  height: 15px;
}
.pi-navigation .close-menu span:after {
  width: 15px;
  height: 1px;
}
.pi-navigation .close-menu span:hover:before, .pi-navigation .close-menu span:hover:after {
  background-color: #e75f4d;
}
.pi-navigation .sub-menu {
  position: absolute;
  width: 250px;
  background-color: #fff;
  left: 0;
  opacity: 0;
  visibility: hidden;
  -webkit-transform: rotateX(90deg);
  -moz-transform: rotateX(90deg);
  -ms-transform: rotateX(90deg);
  -o-transform: rotateX(90deg);
  transform: rotateX(90deg);
  -webkit-transform-origin: 50% 0;
  -moz-transform-origin: 50% 0;
  -ms-transform-origin: 50% 0;
  -o-transform-origin: 50% 0;
  transform-origin: 50% 0;
  -webkit-transition: all 0.3s cubic-bezier(1, 0.01, 0.29, 0.93);
  -moz-transition: all 0.3s cubic-bezier(1, 0.01, 0.29, 0.93);
  -ms-transition: all 0.3s cubic-bezier(1, 0.01, 0.29, 0.93);
  -o-transition: all 0.3s cubic-bezier(1, 0.01, 0.29, 0.93);
  transition: all 0.3s cubic-bezier(1, 0.01, 0.29, 0.93);
}
.pi-navigation .sub-menu li {
  display: block;
  width: 100%;
}
.pi-navigation .sub-menu li a {
  display: block;
  font-size: 14px;
  border: 0;
  line-height: 50px;
  border: 1px solid #e3e3e3;
  border-top: 0;
}
.pi-navigation .sub-menu li:first-child a {
  font-size: 14px;
  padding: 0 25px;
}
.pi-navigation .sub-menu .sub-menu {
  top: -1px;
  left: 100%;
  margin-left: -1px;
  border-top: 1px solid #e3e3e3;
}
.pi-navigation .sub-menu .left {
  left: 100%;
  margin-left: -1px;
}
.pi-navigation .sub-menu .right {
  left: -100%;
  margin-left: 1px;
}
.pi-navigation .nav-hover li:hover > .sub-menu {
  opacity: 1;
  visibility: visible;
  -webkit-transform: rotateX(0deg);
  -moz-transform: rotateX(0deg);
  -ms-transform: rotateX(0deg);
  -o-transform: rotateX(0deg);
  transform: rotateX(0deg);
}
.pi-navigation .submenu-toggle {
  position: absolute;
  width: 40px;
  height: 40px;
  right: 0;
  background: url("../img/arrow.png") no-repeat 50% 50%;
  background-size: 14px;
  -webkit-transform: translateY(-100%) rotate(180deg);
  -moz-transform: translateY(-100%) rotate(180deg);
  -ms-transform: translateY(-100%) rotate(180deg);
  -o-transform: translateY(-100%) rotate(180deg);
  transform: translateY(-100%) rotate(180deg);
  cursor: pointer;
}
.pi-navigation .back-mb a {
  position: absolute;
  height: 40px;
  right: 0;
  background: url("../img/arrow.png") no-repeat 25px 50% !important;
  background-size: 18px !important;
  cursor: pointer;
}

.search-box {
  position: absolute;
  top: 0;
  right: 15px;
}
.search-box.search-mobile {
  right: 65px;
}
.search-box .toggle-search {
  cursor: pointer;
  display: inline-block;
}
.search-box .toggle-search .fa {
  line-height: 62px;
  -webkit-transition: all 0.3s ease;
  -moz-transition: all 0.3s ease;
  -ms-transition: all 0.3s ease;
  -o-transition: all 0.3s ease;
  transition: all 0.3s ease;
}
.search-box .toggle-search:hover .fa {
  color: #e75f4d;
}
.search-box .search-form {
  position: absolute;
  padding: 20px;
  width: 270px;
  border-top: 2px solid #e75f4d;
  right: 0;
  background-color: #fff;
  -webkit-box-shadow: 0 2px 10px 0px rgba(0, 0, 0, 0.2);
  -moz-box-shadow: 0 2px 10px 0px rgba(0, 0, 0, 0.2);
  box-shadow: 0 2px 10px 0px rgba(0, 0, 0, 0.2);
  opacity: 0;
  visibility: hidden;
  -webkit-transform: translateY(20px);
  -moz-transform: translateY(20px);
  -ms-transform: translateY(20px);
  -o-transform: translateY(20px);
  transform: translateY(20px);
  -webkit-transition: all 0.3s ease;
  -moz-transition: all 0.3s ease;
  -ms-transition: all 0.3s ease;
  -o-transition: all 0.3s ease;
  transition: all 0.3s ease;
}
.search-box .search-form form {
  position: relative;
}
.search-box .search-form input {
  width: 100%;
  height: 38px;
  line-height: 38px;
  background-color: #fff;
  border: 1px solid #d7d7d7;
  padding: 0 40px 0 20px;
  -webkit-box-shadow: inset 0 0 20px 0px rgba(0, 0, 0, 0.05);
  -moz-box-shadow: inset 0 0 20px 0px rgba(0, 0, 0, 0.05);
  box-shadow: inset 0 0 20px 0px rgba(0, 0, 0, 0.05);
}
.search-box .search-form .form-actions {
  position: absolute;
  top: 0;
  right: 0;
}
.search-box .search-form .form-actions button {
  background: none;
  border: 0;
  width: 38px;
  height: 38px;
}
.search-box.active .toggle-search .fa {
  color: #e75f4d;
}
.search-box.active .search-form {
  opacity: 1;
  visibility: visible;
  -webkit-transform: translateY(0);
  -moz-transform: translateY(0);
  -ms-transform: translateY(0);
  -o-transform: translateY(0);
  transform: translateY(0);
}

@-webkit-keyframes nav-anim {
  0% {
    top: -100px;
  }

  100% {
    top: 0;
  }
}
@-moz-keyframes nav-anim {
  0% {
    top: -100px;
  }

  100% {
    top: 0;
  }
}
@-ms-keyframes nav-anim {
  0% {
    top: -100px;
  }

  100% {
    top: 0;
  }
}
@-o-keyframes nav-anim {
  0% {
    top: -100px;
  }

  100% {
    top: 0;
  }
}
@keyframes nav-anim {
  0% {
    top: -100px;
  }

  100% {
    top: 0;
  }
}
/* 4.   Story
--------------------------------------------------------------------------------*/
.story .bg-static ~ div,
.story .bg-parallax ~ div {
  color: #fff;
}
.story .bg-static ~ div *,
.story .bg-parallax ~ div * {
  color: inherit;
}
.story .bg-static ~ div .wellcome .sub-title,
.story .bg-parallax ~ div .wellcome .sub-title {
  color: #fff;
}
.story .wellcome .h2 {
  color: #e75f4d;
}
.story .wellcome .sub-title {
  font-family: "Satisfy", cursive;
  font-size: 24px;
  color: #474747;
}
.story .st-heading {
  margin-top: 60px;
  margin-bottom: 20px;
}
.story .st-slogan {
  font-weight: 600;
  font-size: 22px;
  margin-top: 20px;
  margin-bottom: 20px;
}
.story .story-item .image-wrap {
  width: 100%;
}

/* 5.   Skill
--------------------------------------------------------------------------------*/
.skill .bg-static ~ div,
.skill .bg-parallax ~ div {
  color: #fff;
}
.skill .bg-static ~ div *,
.skill .bg-parallax ~ div * {
  color: inherit;
}
.skill .bg-static ~ div .st-body,
.skill .bg-parallax ~ div .st-body {
  font-size: 0;
}
.skill .bg-static ~ div .st-heading .sub-title,
.skill .bg-parallax ~ div .st-heading .sub-title {
  color: inherit;
}
.skill .bg-static ~ div .percent,
.skill .bg-parallax ~ div .percent {
  color: inherit;
}

.skill-item {
  display: inline-block;
}
.skill-item .h5 {
  font-family: "Open Sans", sans-serif;
  font-weight: 600;
}

.skill-bar {
  position: relative;
